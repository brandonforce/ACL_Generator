description "MTNDubai-NPS"
           entry 2 create
               description "Allow-BGP"
               match protocol tcp
                   dst-ip ip-prefix-list "MTN-BGP"
                   dst-port eq 179
               exit
               action forward
           exit
           entry 3 create
               description "Allow-bgp"
               match protocol tcp
                   dst-ip ip-prefix-list "MTN-BGP"
                   src-port eq 179
               exit
               action forward
           exit
           entry 4 create
               description "Block-all"
               match protocol *
                   dst-ip ip-prefix-list "MTN-BGP"
               exit
               action drop
           exit
           entry 5 create
               match protocol udp
                   fragment true
               exit
               action drop
           exit
           entry 6 create
               description "Block-hole"
               match
                   dst-ip ip-prefix-list "MTN-Null-Route"
               exit
               action drop
           exit
           entry 7 create
               description "block-RPC"
               match protocol *
                   port eq 111
               exit
               action drop
           exit
           entry 8 create
               description "Allow-ports-4500-500"
